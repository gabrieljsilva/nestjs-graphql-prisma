model Credentials {
  id       String            @id @default(uuid())
  email    String            @unique
  password String
  user     User              @relation(fields: [user_id], references: [id], onDelete: Cascade)
  user_id  String            @unique
  status   CredentialsStatus

  @@map("credentials")
}

enum CredentialsStatus {
  ACTIVE
  INACTIVE
  WAITING_CONFIRMATION
}
